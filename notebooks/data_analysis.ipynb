import pandas as pdimport seaborn as snsimport matplotlib.pyplot as pltdf = pd.read_csv("../data/school_data_cleaned.csv")# Pie chart of school typesdf['school_type'].value_counts().plot.pie(autopct='%1.1f%%')plt.title("School Type Distribution")plt.ylabel("")plt.savefig("../visuals/school_type_pie.png")plt.show()# Bar chart for top 10 citiestop_cities = df['city'].value_counts().head(10)top_cities.plot(kind='bar')plt.title("Top 10 Cities by Number of Schools")plt.xlabel("City")plt.ylabel("Count")plt.xticks(rotation=45)plt.tight_layout()plt.savefig("../visuals/top_cities_bar.png")plt.show()